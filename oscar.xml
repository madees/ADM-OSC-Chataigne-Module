<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<!-- 

	//==============================================================================
	This file is used to specify the automation parameters of the OSCar plugin.

	It implements the ADM-OSC v1.0 parameters.
	See https://immersive-audio-live.github.io/ADM-OSC/ to know more about it.
	
	//==============================================================================
	
-->
 
<oscar version="1.0">	
    <parameter index="1" name="azim" pattern="/adm/obj/$1/azim" min="-180" max="180" default="0"/>
    <parameter index="2" name="elev" pattern="/adm/obj/$1/elev" min="-90" max="90" default="0"/>
    <parameter index="3" name="dist" pattern="/adm/obj/$1/dist" min="0" max="1" default="1"/>
    <parameter index="4" name="x" pattern="/adm/obj/$1/x" min="-1" max="1" default="0"/>
    <parameter index="5" name="y" pattern="/adm/obj/$1/y" min="-1" max="1" default="0"/>
    <parameter index="6" name="z" pattern="/adm/obj/$1/z" min="-1" max="1" default="0"/>
    <parameter index="7" name="w" pattern="/adm/obj/$1/w" min="0" max="1" default="0"/>
    <parameter index="8" name="gain" pattern="/adm/obj/$1/gain" min="0" max="1" default="1"/>
    <parameter index="9" name="dref" pattern="/adm/obj/$1/dref" min="0" max="1000" default="0"/>
    <parameter index="10" name="dmax" pattern="/adm/obj/$1/dmax" min="0" max="1" default="1"/>
    <parameter index="11" name="mute" pattern="/adm/obj/$1/mute" min="0" max="1" default="0"/>
</oscar>
 
